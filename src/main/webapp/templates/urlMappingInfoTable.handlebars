<thead>
  <tr>
    <th>Short URL</th>
    <th>Redirect URL</th>
    <th>Action</th>
  </tr>
</thead>
<tbody>
  {{#each this}}
    <tr id="row-{{shortUrl}}" data-shorturl="{{shortUrl}}">
      <td><a href="http://localhost:8080/a/{{shortUrl}}">{{shortUrl}}</a></td>
      <td><a href="{{redirectUrl}}">{{redirectUrl}}</a></td>
      <td>
        <button type="button" class="btn btn-danger" data-shorturl="{{shortUrl}}" data-toggle="modal" data-target="#confirm-delete-modal">
          <span class="glyphicon glyphicon-trash" aria-hidden="true"></span> Delete
        </button>
      </td>
    </tr>
  {{/each}}
</tbody>
